<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->


    <style name="Toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">@color/material_dynamic_neutral20</item>
        <item name="theme">@style/ThemeOverlay.Material3.Dark</item>
    </style>

    <style name="BottomNavigation">
        <item name="theme">@style/ThemeOverlay.Material3.Dark</item>
        <item name="android:background">@color/material_dynamic_neutral20</item>
    </style>

    <style name="MainFragmentStyle">
        <item name="android:background">@color/material_dynamic_neutral40</item>
        <item name="android:padding">8dp</item>
    </style>
</resources>